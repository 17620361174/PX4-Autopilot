#!nsh
#
# @name Alta Pro 8
#
# @type Octorotor x
# @class Copter
#

sh /etc/init.d/8001_octo_x

if [ $AUTOCNF == yes ]
then
	param set MC_ROLL_P 7.0
	param set MC_ROLLRATE_P 0.16
	param set MC_ROLLRATE_I 0.05
	param set MC_ROLLRATE_D 0.003
	param set MC_PITCH_P 7.0
	param set MC_PITCHRATE_P 0.16
	param set MC_PITCHRATE_I 0.05
	param set MC_PITCHRATE_D 0.003
	param set MC_YAW_P 2.8
	param set MC_YAWRATE_P 0.3
	param set MC_YAWRATE_I 0.1
	param set MC_YAWRATE_D 0.0
fi

# Required
unset MIXER_AUX octo_x
unset PWM_AUX_RATE 400
unset PWM_AUX_OUT 1234

# override mixer
set MIXER octo_x

# Start FF_CAN module
FF_CAN start


# FF start, customize output rates for all packets
mavlink start -d /dev/ttyS5 -b 57600 -m osd -r 5000

mavlink stream -d /dev/ttyS5 -r 2 -s HEARTBEAT
mavlink stream -d /dev/ttyS5 -r 2 -s SYS_STATUS
mavlink stream -d /dev/ttyS5 -r 0.5 -s SYSTEM_TIME
mavlink stream -d /dev/ttyS5 -r 1 -s GPS_RAW_INT
mavlink stream -d /dev/ttyS5 -r 1 -s GLOBAL_POSITION_INT
mavlink stream -d /dev/ttyS5 -r 25 -s ATTITUDE
mavlink stream -d /dev/ttyS5 -r 10 -s VFR_HUD
mavlink stream -d /dev/ttyS5 -r 2 -s ALTITUDE
mavlink stream -d /dev/ttyS5 -r 1 -s BATTERY_STATUS
mavlink stream -d /dev/ttyS5 -r 0 -s ESTIMATOR_STATUS
mavlink stream -d /dev/ttyS5 -r 0 -s WIND_COV
mavlink stream -d /dev/ttyS5 -r 0 -s VIBRATION
mavlink stream -d /dev/ttyS5 -r 0 -s SERVO_OUTPUT_RAW
mavlink stream -d /dev/ttyS5 -r 2 -s EXTENDED_SYS_STATE
mavlink stream -d /dev/ttyS5 -r 5 -s LOCAL_POSITION_NED
mavlink stream -d /dev/ttyS5 -r 5 -s POSITION_TARGET_LOCAL_NED

